@article{_latex_2015,
	title = {{定制科技期刊LaTeX论文模板的思路和实践}},
	issn = {1001-7143},
	url = {http://kns.cnki.net/kns/detail/detail.aspx?QueryID=6&CurRec=1&recid=&FileName=JYKQ201509016&DbName=CJFDLAST2015&DbCode=CJFQ&yx=&pr=&URLID=},
	abstract = {【目的】为科技期刊量身定制简洁高效的La Te X模版,提高编辑工作质量和效率并指导作者规范写作。【方法】结合编辑部多年La Te X模板的使用经验,通过实践总结,提出新一代科技期刊论文模板的设计理念和方法。通过与其他模版的对比,介绍了新模版实现的功能和优势。【结果】设计的La Te X模版使作者自行排版成为可能,使作者容易上手,并指导作者规范写作,同时大大提高编辑工作效率。【结论】量身定制科技期刊La Te X论文模版,能提高编辑工作质量和效率,并能指导作者提高论文质量。},
	language = {中文;},
	number = {09 vo 26},
	urldate = {2018-09-04},
	journal = {中国科技期刊研究},
	author = {龙, 静 and 尚, 婵妤 and 孙, 云志},
	year = {2015},
	keywords = {定制, 科技期刊, 排版, Customizing, LaTeX template, LaTeX模板, Scientific journals, Typesetting},
	pages = {975--980}
}

